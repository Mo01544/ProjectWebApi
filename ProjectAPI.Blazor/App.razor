<Router AppAssembly="@typeof(Program).Assembly" PreferExactMatches="@true">
    <Found Context="routeData">
        <RouteView RouteData="@routeData" DefaultLayout="@typeof(PublicLayout)" />
    </Found>
    <NotFound>
        <LayoutView Layout="@typeof(PublicLayout)">
            <AllProductsUser/>
        </LayoutView>
    </NotFound>
</Router>
